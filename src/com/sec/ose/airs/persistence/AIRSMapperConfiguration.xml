<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
		"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="useGeneratedKeys" value="true" />
	</settings>
	
	<typeAliases>
		<typeAlias type="com.sec.ose.airs.domain.autoidentify.SPDXPackageDTO" alias="SPDXPackage" />
		<typeAlias type="com.sec.ose.airs.domain.autoidentify.SPDXFileDTO" alias="SPDXFile" />
		<typeAlias type="com.sec.ose.airs.domain.autoidentify.SPDXOtherLicenseDTO" alias="OtherLicense" />
	</typeAliases>

	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC" />
			<dataSource type="POOLED">
				<property name="driver" value="org.sqlite.JDBC" />
				<property name="url" value="jdbc:sqlite:ldbAIRS.db" />
			</dataSource>
		</environment>
	</environments>
	
	<mappers>
		<mapper resource="com/sec/ose/airs/persistence/SPDXMapper.xml" />
	</mappers>
	
</configuration>